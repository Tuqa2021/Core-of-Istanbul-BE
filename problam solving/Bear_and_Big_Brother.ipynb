        "def main():\n",
        "    a, b = map(int, input().split())\n",
        "    res = 0\n",
        "    while a <= b:\n",
        "        a *= 3\n",
        "        b *= 2\n",
        "        res += 1\n",
        "    print(res)\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
